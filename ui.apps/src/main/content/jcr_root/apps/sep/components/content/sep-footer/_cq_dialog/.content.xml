<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Footer"
    width="777px"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <container
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/container"
                margin="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                	<tabs
		                jcr:primaryType="nt:unstructured"
		                sling:resourceType="granite/ui/components/coral/foundation/tabs"
		                maximized="{Boolean}true">
		                <items jcr:primaryType="nt:unstructured">
		                	<footerItems
				          		jcr:primaryType="nt:unstructured"
				    			jcr:title="Itens do Footer"
				            	sling:resourceType="granite/ui/components/coral/foundation/container"
				            	margin="{Boolean}true">
				            	<items jcr:primaryType="nt:unstructured">
		                			<multifield
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
										composite="{Boolean}true" 
										eaem-show-on-collapse="EAEM.showProductName"
										fieldLabel="Lista de itens do footer">
										<field 
											jcr:primaryType="nt:unstructured"
											sling:resourceType="granite/ui/components/coral/foundation/container"
											name="./footerList">
											<items jcr:primaryType="nt:unstructured">
												<column
													granite:class="foundation-layout-util-maximized-alt coral-Form-fieldwrapper"
													jcr:primaryType="nt:unstructured"
													sling:resourceType="granite/ui/components/coral/foundation/container">
													<items jcr:primaryType="nt:unstructured">
														<heading
				                							granite:class="att-dialog-heading"
															jcr:primaryType="nt:unstructured"
															sling:resourceType="granite/ui/components/coral/foundation/heading"
															level="{Long}4" 
															text="Item da lista"/>
														<text 
															jcr:primaryType="nt:unstructured"
															sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
															fieldLabel="Texto" 
															required="{Boolean}true"
															name="./footerText"/>
														<link 
															jcr:primaryType="nt:unstructured"
															sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
															fieldLabel="URL do Link" 
															name="./footerLink" 
															required="{Boolean}true"
															rootPath="/content/sep"/>
														<target 
															jcr:primaryType="nt:unstructured"
															sling:resourceType="granite/ui/components/coral/foundation/form/select"
															fieldLabel="Abrir ao clicar" 
															name="./footerTarget">
															<items jcr:primaryType="nt:unstructured">
																<blank 
																	jcr:primaryType="nt:unstructured"
																	text="Abrir em uma nova aba" 
																	value="_blank"
																	selected="{Boolean}true"/>
																<self 
																	jcr:primaryType="nt:unstructured" 
																	text="Abrir na mesma aba"
																	value="_self"/>
															</items>
														</target>
													</items>
												</column>
											</items>
										</field>
									</multifield>
		                		</items>
		                	</footerItems>
		                	<social
				          		jcr:primaryType="nt:unstructured"
				    			jcr:title="Mídias Sociais"
				            	sling:resourceType="granite/ui/components/coral/foundation/container"
				            	margin="{Boolean}true">
				            	<items jcr:primaryType="nt:unstructured">
		                			<multifield
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
										composite="{Boolean}true" 
										eaem-show-on-collapse="EAEM.showProductName"
										fieldLabel="Lista de ícones sociais">
										<field 
											jcr:primaryType="nt:unstructured"
											sling:resourceType="granite/ui/components/coral/foundation/container"
											name="./socialList">
											<items jcr:primaryType="nt:unstructured">
												<column
													granite:class="foundation-layout-util-maximized-alt coral-Form-fieldwrapper"
													jcr:primaryType="nt:unstructured"
													sling:resourceType="granite/ui/components/coral/foundation/container">
													<items jcr:primaryType="nt:unstructured">
														<heading
				                							granite:class="att-dialog-heading"
															jcr:primaryType="nt:unstructured"
															sling:resourceType="granite/ui/components/coral/foundation/heading"
															level="{Long}4" 
															text="Item da lista"/>
														<icons 
															jcr:primaryType="nt:unstructured"
															sling:resourceType="acs-commons/components/authoring/graphiciconselect"
															fieldLabel="Ícones" 
															name="./socialIcon"
															required="{Boolean}true">
															<datasource 
																jcr:primaryType="nt:unstructured"
																sling:resourceType="acs-commons/components/utilities/genericlist/datasource"
																path="/etc/acs-commons/lists/font-awesome-icons"/>
														</icons>
														<link 
															jcr:primaryType="nt:unstructured"
															sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
															fieldLabel="URL do Link" 
															name="./socialLink" 
															required="{Boolean}true"
															rootPath="/content/sep"/>
														<target 
															jcr:primaryType="nt:unstructured"
															sling:resourceType="granite/ui/components/coral/foundation/form/select"
															fieldLabel="Abrir ao clicar" 
															name="./socialTarget">
															<items jcr:primaryType="nt:unstructured">
																<blank 
																	jcr:primaryType="nt:unstructured"
																	text="Abrir em uma nova aba" 
																	value="_blank"
																	selected="{Boolean}true"/>
																<self 
																	jcr:primaryType="nt:unstructured" 
																	text="Abrir na mesma aba"
																	value="_self"/>
															</items>
														</target>
													</items>
												</column>
											</items>
										</field>
									</multifield>
		                		</items>
		                	</social>
		                	<copyright
				          		jcr:primaryType="nt:unstructured"
				    			jcr:title="Direito Autoral"
				            	sling:resourceType="granite/ui/components/coral/foundation/container"
				            	margin="{Boolean}true">
				            	<items jcr:primaryType="nt:unstructured">
									<copyright 
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										value="Copyright © Sociedade Esportiva Palmeiras | Política de privacidade"
										fieldLabel="Direito Autoral"
										required="{Boolean}true"
										name="./copyright"/>
								</items>
							</copyright>		
		                </items>
		            </tabs>
				</items>
            </container>
        </items>
    </content>
</jcr:root>