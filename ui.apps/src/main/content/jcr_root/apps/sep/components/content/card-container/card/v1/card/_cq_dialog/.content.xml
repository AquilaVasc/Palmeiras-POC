<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="card"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <column
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <title
                        jcr:primaryType="nt:unstructured"
					    sling:resourceType="granite/ui/components/foundation/include"
					    path="/apps/osf/components/content/reusablefields/cq:dialog/content/items/title"/>
					<description
                        jcr:primaryType="nt:unstructured"
					    sling:resourceType="granite/ui/components/foundation/include"
					    path="/apps/osf/components/content/reusablefields/cq:dialog/content/items/description"/>
					<category
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldLabel="category.label"
                        name="./category"/>
                    <linkto
					    jcr:primaryType="nt:unstructured"
					    sling:resourceType="granite/ui/components/foundation/include"
					    path="/apps/osf/components/content/reusablefields/cq:dialog/content/items/linkto"/>
					<imageurl
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                        fieldLabel="imageurl.label"
                        rootPath="/content/dam/ATT"
                        name="./imageurl"/>
                    <alttext
					    jcr:primaryType="nt:unstructured"
						sling:resourceType="granite/ui/components/foundation/include"
						path="/apps/osf/components/content/reusablefields/cq:dialog/content/items/alttext"/>
                </items>
            </column>
        </items>
    </content>
</jcr:root>
